##Change Every Month to New Folder and CSV File##

setwd("S:/Indiana Research & Evaluation/HRSA Healthy Start/Monthly Client-Level Data - HSMED/November 2019")
hrsa = read.csv("Nov2019Data.csv", header = TRUE)
head(hrsa)
attach(hrsa)

#Download Packages
library(lubridate)
library(tidyverse)
library(tidyr)

#FOR EACH TOOL - When subsetting by the date, ensure that you are selecting the correct month's days. This must be done for each Tool's Recodes step

# Step 1: Demo Tool Recodes
DemoRecodes = hrsa %>%
  mutate(admindate = mdy(admindate)) %>%
  subset(admindate >= "2019-11-01" & admindate <= "2019-11-30") %>%
  mutate(racelist___1 = recode(racelist___1, '1' = "1", '0' = "0")) %>%
  mutate(racelist___2 = recode(racelist___2, '1' = "2", '0' = "0")) %>%
  mutate(racelist___3 = recode(racelist___3, '1' = "3", '0' = "0")) %>%
  mutate(racelist___4 = recode(racelist___4, '1' = "4", '0' = "0")) %>%
  mutate(racelist___5 = recode(racelist___5, '1' = "5", '0' = "0")) %>%
  mutate(racelist___6 = recode(racelist___6, '1' = "6", '0' = "0")) %>%
  mutate(racelist___7 = recode(racelist___7, '1' = "7", '0' = "0")) %>%
  mutate(racelist___8 = recode(racelist___8, '1' = "8", '0' = "0")) %>%
  mutate(racelist___9 = recode(racelist___9, '1' = "9", '0' = "0")) %>%
  mutate(racelist___10 = recode(racelist___10, '1' = "10", '0' = "0")) %>%
  mutate(racelist___11 = recode(racelist___11, '1' = "11", '0' = "0")) %>%
  mutate(racelist___12 = recode(racelist___12, '1' = "12", '0' = "0")) %>%
  mutate(racelist___13 = recode(racelist___13, '1' = "13", '0' = "0")) %>%
  mutate(racelist___14 = recode(racelist___14, '1' = "14", '0' = "0")) %>%
  mutate(racelist___15 = recode(racelist___15, '1' = "15", '0' = "0")) %>%
  mutate(racelist___16 = recode(racelist___16, '1' = "16", '0' = "0")) %>%
  unite(col = RaceIds, racelist___1, racelist___2, racelist___3, racelist___4, racelist___5, racelist___6, racelist___7, racelist___8, racelist___9,racelist___10, racelist___11, racelist___12, racelist___13, racelist___14, racelist___15, racelist___16, sep = ",") %>%
  mutate(activeclient2019 = recode(activeclient2019, '1' = "2019", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2020 = recode(activeclient2020, '1' = "2020", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2021 = recode(activeclient2021, '1' = "2021", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2022 = recode(activeclient2022, '1' = "2022", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2023 = recode(activeclient2023, '1' = "2023", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2024 = recode(activeclient2024, '1' = "2024", '0' = "0", .missing = "0")) %>%
  unite(col = ActiveClient, activeclient2019, activeclient2020, activeclient2021, activeclient2022, activeclient2023, activeclient2024, sep = ";")

#Step 2: Attach new dataset to call without dollarsign
attach(DemoRecodes)

#Step 3: Removes unneeded zeros and commas
DemoRecodes$RaceIds = str_remove_all(RaceIds, ",0|0,|0")
DemoRecodes$ActiveClient = str_remove_all(ActiveClient, ";0|0;|;0;")

#Step 4: Formats dates correctly
DemoRecodes$admindate = format(strptime(admindate, format = "%Y-%m-%d"), "%m/%d/%Y")

#Step 5: Creates dataframe in necessary HRSA Template format
DemoTemplate = DemoRecodes %>%
  subset(select = c(clientuniqueidentification, consentflag, participantcaregiver, ActiveClient, admindate, birthdate, zipcode, highestgradeschoolid, ethnicitylist, ethnicityother, RaceIds, isusbornid, countryborn, countryborndrid, whentous, englishlevelid, otherlanguagespokenid, languageathomeid, africanlanguage, chineselanguage, otherlanguage))

#Step 6: Export Finalized Demographics Template
write.csv(DemoTemplate, file = "DemoTemplateFinal.csv", na = "")

#Step 1: Pregnancy History Tool Recodes
PregHistRecodes = hrsa %>%
  mutate(admindate2 = mdy(admindate2)) %>%
  subset(admindate2 >= "2019-11-01" & admindate2 <= "2019-11-30") %>%
  mutate(pregproblemslist___1 = recode(pregproblemslist___1, '1' = "1", '0' = "0")) %>%
  mutate(pregproblemslist___2 = recode(pregproblemslist___2, '1' = "2", '0' = "0")) %>%
  mutate(pregproblemslist___3 = recode(pregproblemslist___3, '1' = "3", '0' = "0")) %>%
  mutate(pregproblemslist___4 = recode(pregproblemslist___4, '1' = "4", '0' = "0")) %>%
  mutate(pregproblemslist___5 = recode(pregproblemslist___5, '1' = "5", '0' = "0")) %>%
  mutate(pregproblemslist___6 = recode(pregproblemslist___6, '1' = "6", '0' = "0")) %>%
  mutate(pregproblemslist___7 = recode(pregproblemslist___7, '1' = "7", '0' = "0")) %>%
  mutate(pregproblemslist___8 = recode(pregproblemslist___8, '1' = "8", '0' = "0")) %>%
  mutate(pregproblemslist___9 = recode(pregproblemslist___9, '1' = "9", '0' = "0")) %>%
  mutate(pregproblemslist___10 = recode(pregproblemslist___10, '1' = "10", '0' = "0")) %>%
  mutate(pregproblemslist___11 = recode(pregproblemslist___11, '1' = "11", '0' = "0")) %>%
  mutate(pregproblemslist___12 = recode(pregproblemslist___12, '1' = "12", '0' = "0")) %>%
  mutate(pregproblemslist___13 = recode(pregproblemslist___13, '1' = "13", '0' = "0")) %>%
  unite(col = PregProblemsIds, pregproblemslist___1, pregproblemslist___2, pregproblemslist___3, pregproblemslist___4, pregproblemslist___5, pregproblemslist___6, pregproblemslist___7, pregproblemslist___8, pregproblemslist___9,pregproblemslist___10, pregproblemslist___11, pregproblemslist___12, pregproblemslist___13, sep = ",") %>%
  mutate(activeclient2019 = recode(activeclient2019, '1' = "2019", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2020 = recode(activeclient2020, '1' = "2020", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2021 = recode(activeclient2021, '1' = "2021", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2022 = recode(activeclient2022, '1' = "2022", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2023 = recode(activeclient2023, '1' = "2023", '0' = "0", .missing = "0")) %>%
  mutate(activeclient2024 = recode(activeclient2024, '1' = "2024", '0' = "0", .missing = "0")) %>%
  unite(col = ActiveClient, activeclient2019, activeclient2020, activeclient2021, activeclient2022, activeclient2023, activeclient2024, sep = ";")

#Step 2: Attach new dataset to call without dollarsign
attach(PregHistRecodes)

#Step 3: Removes unneeded zeros and commas
PregHistRecodes$PregProblemsIds = str_remove_all(PregProblemsIds, ",0|0,|0")
PregHistRecodes$ActiveClient = str_remove_all(ActiveClient, ";0|0;|;0;")

#Step 4: Formats dates correctly
PregHistRecodes$admindate2 = format(strptime(admindate2, format = "%Y-%m-%d"), "%m/%d/%Y")

#Step 5: Creates dataframe in necessary HRSA Template format

PregHistTemplate = PregHistRecodes %>%
  subset(select = c(clientuniqueidentification, consentflag, participantcaregiver, ActiveClient, admindate2, ispregnantid, weekpregnantcount, monthpregnantcount, weekmonthcountdrid, beenpregnantincludingcount, beenpregnantincludingcountdrid, prevpregnancydeclined, hadcsectionid, complicationpregid, PregProblemsIds, otherpregproblem, bornthreeweeksid, bornthreeweekscount, lessthanfivepoundsid, lessthanfivepoundscount, babystayedinhospitalid, babystateinhospitalreason, childrenlivewithyouid))

#Step 6: Export Pregnancy History Template
write.csv(PregHistTemplate, file = "PregHistTemplateFinal.csv", na = "")

#Step 7: Pregnancy History Previous Pregnancy Template creation

PHPrevPregTemplate = hrsa %>%
  mutate(admindate2 = mdy(admindate2)) %>%
  subset(admindate2 >= "2019-11-01" & admindate2 <= "2019-11-30") %>%
  subset(select = c(clientuniqueidentification, numoflivebirth, dateoflastbirth, dateofmiscarriage, dateoftubepreg, dateofabortion, numofstillbirth, dateofstillbirth, clientuniqueidentification, numoflivebirth_2, dateoflastbirth_2, dateofmiscarriage_2, dateoftubepreg_2, dateofabortion_2, numofstillbirth_2, dateofstillbirth_2, clientuniqueidentification, numoflivebirth_3, dateoflastbirth_3, dateofmiscarriage_3, dateoftubepreg_3, dateofabortion_3, numofstillbirth_3, dateofstillbirth_3, clientuniqueidentification, numoflivebirth_4, dateoflastbirth_4, dateofmiscarriage_4, dateoftubepreg_4, dateofabortion_4, numofstillbirth_4, dateofstillbirth_4, clientuniqueidentification, numoflivebirth_5, dateoflastbirth_5, dateofmiscarriage_5, dateoftubepreg_5, dateofabortion_5, numofstillbirth_5, dateofstillbirth_5))

#Step 8: Export Previous Pregnancy Template
write.csv(PHPrevPregTemplate, file = "PHPrevPregTemplateFinal.csv", na = "")


