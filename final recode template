#Step 1: Parenting Template Recodes
ParentingRecodes = hrsa %>%
  mutate(admindate_parenting = mdy(admindate_parenting)) %>%
  subset(admindate_parenting >= "2019-11-01" & admindate_parenting <= "2019-11-30") %>%
  mutate(fuchilddevinfolist_parenting___1 = recode(fuchilddevinfolist_parenting___1, '1' = "1", '0' = "0")) %>%
  mutate(fuchilddevinfolist_parenting___2 = recode(fuchilddevinfolist_parenting___2, '1' = "2", '0' = "0")) %>%
  unite(col = FUChildDevInfoIds_Parenting, fuchilddevinfolist_parenting___1, fuchilddevinfolist_parenting___2, sep = ",") %>%
  mutate(fuchilddevreferredlist_parenting___1 = recode(fuchilddevreferredlist_parenting___1, '1' = "1", '0' = "0")) %>%
  mutate(fuchilddevreferredlist_parenting___2 = recode(fuchilddevreferredlist_parenting___2, '1' = "2", '0' = "0")) %>%
  mutate(fuchilddevreferredlist_parenting___3 = recode(fuchilddevreferredlist_parenting___3, '1' = "3", '0' = "0")) %>%
  mutate(fuchilddevreferredlist_parenting___4 = recode(fuchilddevreferredlist_parenting___4, '1' = "4", '0' = "0")) %>%
  unite(col = FUChildDevReferredIds_Parenting, fuchilddevreferredlist_parenting___1, fuchilddevreferredlist_parenting___2, fuchilddevreferredlist_parenting___3, fuchilddevreferredlist_parenting___4, sep = ",") %>%
