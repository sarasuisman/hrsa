attach(PrenatRecodes)

#Step 3: Deletes all unneeded zeros and commas from string variables
PrenatRecodes$PrenatalCareDifficultReasonIds_Prenatal = str_remove_all(PrenatalCareDifficultReasonIds_Prenatal, ",0|0,|0")
PrenatRecodes$HealthInsuranceTypesIds_Prenatal = str_remove_all(HealthInsuranceTypesIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUEnrolledInMedicaidInfoIds_Prenatal = str_remove_all(FUEnrolledInMedicaidInfoIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUEnrolledInMedicaidReferredIds_Prenatal = str_remove_all(FUEnrolledInMedicaidReferredIds_Prenatal, ",0|0,|0")
PrenatRecodes$ConcernHousingIds_Prenatal = str_remove_all(ConcernHousingIds_Prenatal, ",0|0,|0")  
PrenatRecodes$FUServiceInfoGivenIds_Prenatal = str_remove_all(FUServiceInfoGivenIds_Prenatal, ",0|0,|0")  
PrenatRecodes$FUServiceReferredIds_Prenatal = str_remove_all(FUServiceReferredIds_Prenatal, ",0|0,|0")  
PrenatRecodes$MedicalConditionsPast6MonthsIds_Prenatal = str_remove_all(MedicalConditionsPast6MonthsIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUHealthInfoIds_Prenatal = str_remove_all(FUHealthInfoIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUHealthProvidedIds_Prenatal = str_remove_all(FUHealthProvidedIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUHealthReferredIds_Prenatal = str_remove_all(FUHealthReferredIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUDepressionInfoIds_Prenatal = str_remove_all(FUDepressionInfoIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUDepressionProvidedIds_Prenatal = str_remove_all(FUDepressionProvidedIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUDepressionReferredIds_Prenatal = str_remove_all(FUDepressionReferredIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUSubstanceUseInfoIds_Prenatal = str_remove_all(FUSubstanceUseInfoIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUSubstanceUseProvidedTNPIds_Prenatal = str_remove_all(FUSubstanceUseProvidedTNPIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUSubstanceUseReferredIds_Prenatal = str_remove_all(FUSubstanceUseReferredIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUPersonalSafetyInfoIds_Prenatal = str_remove_all(FUPersonalSafetyInfoIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUStressReferredIds_Prenatal = str_remove_all(FUStressReferredIds_Prenatal, ",0|0,|0")
PrenatRecodes$FatherRoleIds_Prenatal = str_remove_all(FatherRoleIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUSupportReferredIds_Prenatal = str_remove_all(FUSupportReferredIds_Prenatal, ",0|0,|0")
PrenatRecodes$FUBirthControlProvidedIds_Prenatal = str_remove_all(FUBirthControlProvidedIds_Prenatal, ",0|0,|0")
PrenatRecodes$ActiveClient = str_remove_all(ActiveClient, ";0|0;|;0;")

#Step 4: Converts date from 2019-02-01 format to proper 02/01/2019 format for HRSA template
PrenatRecodes$admindate_prenatal = format(strptime(admindate_prenatal, format = "%Y-%m-%d"), "%m/%d/%Y")

#Step 5: Create Prenatal HSMED Template and Export to Proper Folder
